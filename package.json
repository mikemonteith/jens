{
  "name": "jens",
  "version": "0.0.1",
  "description": "A static site generator",
  "main": "main.js",
  "scripts": {
    "build-component-library": "electron ./support/component-library/main.js",
    "build-component-library-js": "webpack --config ./support/component-library/webpack.config.js",
    "build-component-library-js-watch": "webpack -w --config ./support/component-library/webpack.config.js",
    "build-docs": "documentation build main.js app/index.js -f html -o docs",
    "build-electron": "NODE_ENV=dev electron -r babel-register ./main.js",
    "build-js": "webpack --config webpack.config.js",
    "build-js-watch": "webpack -w --config webpack.config.js",
    "component-library": "npm run build-component-library & npm run build-component-library-js-watch",
    "lint": "eslint .",
    "start": "npm run build-electron & npm run build-js-watch",
    "test": "NODE_ENV=test mocha test"
  },
  "author": "Mike Monteith <mike@mikemonteith.com>",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.2",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "directory-tree": "^2.1.0",
    "draft-js": "^0.10.5",
    "electron": "^2.0.5",
    "electron-devtools-installer": "^2.2.4",
    "node-sass": "^4.1.1",
    "nodegit": "^0.16.0",
    "npm": "^4.0.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-mde": "^5.7.0",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-electron-store": "^0.4.1",
    "redux-electron-windows": "0.0.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.4.0",
    "redux-persist-electron-storage": "^1.1.2",
    "redux-saga": "^0.14.1",
    "reselect": "^3.0.1",
    "sass-loader": "^4.1.1",
    "showdown": "^1.8.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-loader": "0.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "documentation": "^6.0.0",
    "electron-rebuild": "^1.5.7",
    "eslint": "^3.12.2",
    "eslint-config-react-app": "^0.5.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "mocha": "^5.2.0",
    "spectron": "^3.8.0"
  }
}
